    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "chai": "^4.2.0",
    "eslint": "^8.51.0",
    "ethers": "^6.8.0",
    "hardhat": "^2.18.1",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.43",
    "hardhat-deploy-ethers": "^0.4.1",
