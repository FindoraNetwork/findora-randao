{
  "name": "eth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "yarn hardhat compile",
    "type": "yarn hardhat typechain",
    "build": "run-s compile type",
    "lint:ts": "eslint 'scripts/*.ts' 'hardhat.config.ts'",
    "lint:tsfix": "eslint 'scripts/*.ts 'hardhat.config.ts'' --fix",
    "lint:sol": "solhint 'contracts/*.sol'",
    "lint:solfix": "solhint 'contracts/*.sol'  --fix",
    "fmt:ts": "prettier --write 'scripts/*.ts' 'hardhat.config.ts'",
    "fmt:sol": "prettier --write 'contracts/*.sol'",
    "fmt": "run-s fmt:ts fmt:sol",
    "lint": "run-s lint:ts lint:sol",
    "lint:fix": "run-s lint:tsfix lint:solfix",
    "clean": "rm -rf build",
    "hardhat:hyprtest": "hardhat --network hyprtest",
    "dep:hyprtest": "yarn hardhat:hyprtest run scripts/deploy.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
